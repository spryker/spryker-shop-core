{% extends model('template') %}

{% define data = {
    form: _view.form,
    products: _view.products,
    additionalColumns: _view.additionalColumns,
    prices: _view.prices,
    extraSearchFormElementClassName: 'js-quick-order-form__search-form-element',
} %}

{% block template %}
    {% include molecule('quick-order-rows', 'QuickOrderPage') with {
        data: {
            rows: data.form.items,
            products: data.products,
            additionalColumns: data.additionalColumns,
            prices: data.prices,
            extraSearchFormElementClassName: data.extraSearchFormElementClassName,
        },
    } only %}
{% endblock %}
