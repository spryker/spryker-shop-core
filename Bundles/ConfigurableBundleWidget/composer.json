{
  "name": "spryker-shop/configurable-bundle-widget",
  "type": "library",
  "description": "ConfigurableBundleWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker-shop/shop-application": "^1.2.0",
    "spryker-shop/shop-ui": "^1.29.0",
    "spryker/configurable-bundle": "^1.0.0",
    "spryker/configurable-bundle-cart": "^1.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/quote": "^2.8.0",
    "spryker/symfony": "^3.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker-shop/configurable-bundle-cart-note-widget": "*",
    "spryker/router": "*",
    "spryker/silex": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/router": "Use this module if you want use the RouterProviderPlugin.",
    "spryker/silex": "Use this module when using plugins that need Silex dependencies.",
    "spryker-shop/configurable-bundle-cart-note-widget": "If you want to use configurable bundle cart note feature."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
