{
    "name": "spryker-shop/sales-product-configuration-widget",
    "type": "library",
    "description": "SalesProductConfigurationWidget module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker-shop/customer-reorder-widget-extension": "^1.1.0",
        "spryker-shop/sales-product-configuration-widget-extension": "^0.1.0",
        "spryker-shop/shop-ui": "^1.54.0",
        "spryker/kernel": "^3.33.0",
        "spryker/sales-product-configuration": "^0.1.1"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
