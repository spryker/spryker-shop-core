<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="QuoteCollection">
        <property name="quotes" type="Quote[]" singular="quote"/>
    </transfer>

    <transfer name="Quote">
        <property name="isDefault" type="bool"/>
        <property name="idQuote" type="int"/>
        <property name="isLocked" type="bool"/>
        <property name="totalItemCount" type="int" strict="true"/>
    </transfer>

    <transfer name="ProductView">
    </transfer>

    <transfer name="MiniCartView" strict="true">
        <property name="content" type="string"/>
        <property name="counterOnly" type="bool"/>
    </transfer>

</transfers>
