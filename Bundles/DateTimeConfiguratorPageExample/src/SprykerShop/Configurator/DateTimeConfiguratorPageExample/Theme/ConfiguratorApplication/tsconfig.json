/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
    "compileOnSave": false,
    "compilerOptions": {
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "removeComments": true,
        "useDefineForClassFields": false,
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "moduleResolution": "node",
        "target": "es2020",
        "module": "esnext",
        "typeRoots": ["node_modules/@types"],
        "lib": ["dom", "esnext"]
    }
}
