{
  "name": "spryker-shop/date-time-configurator-page-example",
  "type": "library",
  "description": "DateTimeConfiguratorPageExample module",
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "spryker-shop/product-configuration-widget-extension": "^0.1.0",
    "spryker-shop/sales-product-configuration-widget-extension": "^0.1.0",
    "spryker-shop/shop-ui": "^1.45.0",
    "spryker/availability": "^9.1.0",
    "spryker/availability-extension": "^1.0.0",
    "spryker/checksum-generator": "dev-bugfix/cc-10508/dev-configurable-product-phase2",
    "spryker/kernel": "^3.30.0",
    "spryker/log": "^3.0.0",
    "spryker/product-configuration-extension": "^0.1.0",
    "spryker/sales-product-configuration-gui-extension": "^0.1.0",
    "spryker/symfony": "^3.0.0",
    "spryker/transfer": "^3.18.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/decimal-object": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerTest\\": "tests/SprykerTest/"
    }
  },
  "repositories": [
    {
      "type": "git",
      "url": "git@github.com:spryker/checksum-generator.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "phpstan": "vendor/bin/phpstan.phar analyse -c tests/phpstan.neon -l 7 src/",
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "authors": [
    {
      "name": "Spryker Systems GmbH",
      "homepage": "https://spryker.com"
    }
  ]
}
