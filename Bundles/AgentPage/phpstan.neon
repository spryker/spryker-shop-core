parameters:
    level: 7
    checkMissingIterableValueType: false
    reportUnmatchedIgnoredErrors: false
    checkGenericClassInNonGenericObjectType: false
    ignoreErrors:
        - '#Parameter \#3 \$roles of class Symfony\\Component\\Security\\Core\\Authentication\\Token\\UsernamePasswordToken constructor expects array<string>, string given\.#'
        - '#Class .+UsernamePasswordToken constructor invoked with 4 parameters, 2-3 required.#'
        - '#Call to an undefined method .+getPassword\(\).#'
        - '#Call to an undefined method .+getUsername\(\).#'
        - '#Class .+UsernamePasswordFormAuthenticationListener not found.#'
        - '#Instantiated class .+UsernameNotFoundException not found.#'
        - '#PHPDoc tag @throws with type .+UsernameNotFoundException\|.+UserNotFoundException is not subtype of Throwable#'
        - '#Parameter \#1 \$passport of method .+AgentLoginFormAuthenticator::createToken\(\) expects .+Passport, .+PassportInterface given.#'
        - '#Throwing object of an unknown class .+UsernameNotFoundException.#'
        - '#Call to an undefined method .+AuthenticationSuccessHandlerInterface::executeOnAuthenticationSuccess\(\).#'
