{
    "name": "spryker-shop/merchant-relation-request-widget",
    "type": "library",
    "description": "MerchantRelationRequestWidget module",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "spryker-shop/shop-ui": "^1.73.0",
        "spryker/company-user": "^2.10.0",
        "spryker/kernel": "^3.52.0",
        "spryker/merchant-relation-request": "^1.0.0",
        "spryker/merchant-storage": "^1.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "suggest": {
        "spryker-shop/merchant-relation-request-page": "Use this module to have an access to the links and routes of the merchant relation request page.",
        "spryker/permission": "If you want to use permissions plugins.",
        "spryker/router": "Use this module when you want to use the Router.",
        "spryker/silex": "Use this module when using plugins that need Silex dependencies."
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
