{% extends template('page-critical-path') %}

{% block pageInfo %}{% endblock %}

{% block content %}
    {% include molecule('jumbotron') with {
        class: 'box'
    } only %}

    <div class="box spacing-top spacing-top--big">
        {{ spyCmsBlock({
            name: 'Home Page'
        }) }}
    </div>

    <div class="box spacing-top spacing-top--big">
        {{ spyCmsBlock({
            name: 'Teaser for home page'
        }) }}
    </div>
{% endblock %}

{% block eventTracker %}
    {% set events = {
        list: events.list,
        data: events.data | merge({
            page: 'HOME',
        }),
    } %}

    {{ parent() }}
{% endblock %}
