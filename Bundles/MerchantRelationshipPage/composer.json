{
    "name": "spryker-shop/merchant-relationship-page",
    "type": "library",
    "description": "MerchantRelationshipPage module",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "spryker-shop/shop-application": "^1.0.0",
        "spryker-shop/shop-ui": "^1.73.0",
        "spryker/application": "^3.8.0",
        "spryker/company-business-unit": "^2.0.0",
        "spryker/company-user": "^2.10.0",
        "spryker/kernel": "^3.30.0",
        "spryker/merchant-relationship": "^1.12.0",
        "spryker/merchant-search": "^1.0.0",
        "spryker/merchant-storage": "^1.0.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker-shop/company-page": "*",
        "spryker/code-sniffer": "*",
        "spryker/router": "*"
    },
    "suggest": {
        "spryker-shop/company-page": "Use this module if you want to use 'page-layout-company' template.",
        "spryker/router": "Use this module if you want to use the RouterProviderPlugin."
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
