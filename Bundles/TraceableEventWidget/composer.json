{
    "name": "spryker-shop/traceable-event-widget",
    "type": "library",
    "description": "TraceableEventWidget module",
    "license": "proprietary",
    "require": {
        "php": ">=8.3",
        "spryker-shop/shop-ui": "^1.79.0",
        "spryker-shop/store-widget": "^1.3.0",
        "spryker/kernel": "^3.30.0",
        "spryker/kernel-app": "^1.2.0",
        "spryker/locale": "^4.8.0",
        "spryker/search-http": "^0.5.0",
        "spryker/transfer": "^3.25.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
