{
    "name": "spryker-shop/redirect-page",
    "type": "library",
    "description": "RedirectPage module",
    "license": "proprietary",
    "require": {
        "php": ">=8.1",
        "spryker-shop/shop-application": "^1.0.0",
        "spryker-shop/shop-router-extension": "^1.0.0",
        "spryker-shop/storage-router-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/storage": "^3.4.0",
        "spryker/symfony": "^3.1.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/url-storage": "*"
    },
    "suggest": {
        "spryker/url-storage": "Use this module when using plugins that need UrlStorage dependencies."
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
