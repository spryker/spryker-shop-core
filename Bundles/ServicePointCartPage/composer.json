{
    "name": "spryker-shop/service-point-cart-page",
    "type": "library",
    "description": "ServicePointCartPage module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker-shop/checkout-page-extension": "^1.5.0",
        "spryker/glossary-storage": "^1.5.0",
        "spryker/kernel": "^3.30.0",
        "spryker/locale": "^3.0.0 || ^4.0.0",
        "spryker/messenger": "^3.1.0",
        "spryker/quote": "^2.0.0",
        "spryker/service-point-cart": "^0.3.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerShopTest\\": "tests/SprykerShopTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
