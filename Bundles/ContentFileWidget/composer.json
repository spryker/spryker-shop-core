{
  "name": "spryker-shop/content-file-widget",
  "type": "library",
  "description": "ContentFileWidget module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker-shop/file-manager-widget": "^1.1.0",
    "spryker/content-file": "^1.0.0",
    "spryker/file-manager-storage": "^1.0.0",
    "spryker/kernel": "^3.34.0",
    "spryker/twig": "^3.5.1",
    "spryker/twig-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/container": "*",
    "spryker/testify": "*"
  },
  "suggest": {
    "spryker/container": "If you want to use Twig Plugin"
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SprykerShopTest\\": "tests/SprykerShopTest/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
