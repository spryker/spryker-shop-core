{
  "name": "spryker-shop/checkout-page-extension",
  "type": "library",
  "description": "CheckoutPageExtension module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/step-engine-extension": "^1.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "suggest": {
    "spryker/symfony": "If you want to use CheckoutPage expander plugins."
  },
  "autoload": {
    "psr-4": {
      "SprykerShop\\": "src/SprykerShop/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
