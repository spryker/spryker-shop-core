{
    "name": "spryker-shop/shop-cms-slot",
    "type": "library",
    "description": "ShopCmsSlot module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker-shop/shop-cms-slot-extension": "^1.0.0",
        "spryker/cms-slot": "^1.0.0",
        "spryker/cms-slot-storage": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/transfer": "^3.8.0",
        "spryker/twig": "^3.2.3"
    },
    "require-dev": {
        "spryker-shop/shop-application": "*",
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "suggest": {
        "spryker-shop/shop-application": "If you want to use the CMS slot plugin for Twig."
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerShopTest\\": "tests/SprykerShopTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
