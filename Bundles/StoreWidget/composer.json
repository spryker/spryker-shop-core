{
    "name": "spryker-shop/store-widget",
    "type": "library",
    "description": "StoreWidget module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/application-extension": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/session": "^4.0.0",
        "spryker/store": "^1.19.0",
        "spryker/store-storage": "^1.0.0",
        "spryker/symfony": "^3.11.1",
        "spryker/transfer": "^3.25.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/container": "*"
    },
    "suggest": {
        "spryker/container": "Add this module to use the ContainerInterface."
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
