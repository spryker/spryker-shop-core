<div class="row">
    <div class="small-12 columns">
        <div class="row">

            {% for productSet in _view.productSets %}
                <div class="small-12 medium-6 large-6 xlarge-4 columns">
                    {% include "@ProductSetListPage/_product-set-list-page/_partials/product-set.twig" with {
                        url: productSet.url,
                        name: productSet.name,
                        imageUrl: (productSet.imageSets|length and productSet.imageSets.0.images|length ? productSet.imageSets.0.images.0.externalUrlSmall : '')
                    } %}
                </div>
            {% else %}
                <div class="small-12 columns">
                    <div class="callout warning">
                        {{ 'product.sets.no-results' | trans }}
                    </div>
                </div>
            {% endfor %}
        </div>
    </div>
</div>
