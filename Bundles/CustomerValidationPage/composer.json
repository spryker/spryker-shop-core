{
    "name": "spryker-shop/customer-validation-page",
    "type": "library",
    "description": "CustomerValidationPage module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker-shop/shop-application-extension": "^1.1.0",
        "spryker/customer": "^7.48.0",
        "spryker/customer-storage": "^1.0.0",
        "spryker/kernel": "^3.52.0",
        "spryker/session": "^4.0.0",
        "spryker/symfony": "^3.1.0",
        "spryker/transfer": "^3.27.0"
    },
    "require-dev": {
        "spryker/application": "*",
        "spryker/code-sniffer": "*",
        "spryker/container": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "SprykerShop\\": "src/SprykerShop/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerShopTest\\": "tests/SprykerShopTest/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
